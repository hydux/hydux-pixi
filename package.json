{
  "name": "hydux-data",
  "version": "0.0.0",
  "description": "Data-driven development for hydux, in the Elm way.",
  "main": "./lib/index.js",
  "typings": "./lib/index",
  "types": "./lib/index",
  "scripts": {
    "build": "del lib && tsc",
    "build:dist": "webpack -p --progress --profile",
    "test": "mocha --require ts-node/register \"src/test/**/*.test.ts\"",
    "test:watch": "npm run test -- -w --watch-extensions ts,tsx",
    "doc": "typedoc --theme minimal --out ./docs --exclude ./test ./src && touch ./docs/.nojekyll",
    "preversion": "npm run build && npm run build:dist && git add -A"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydux/hydux-data.git"
  },
  "keywords": [
    "elm",
    "hyperapp",
    "redux",
    "flux",
    "react",
    "picodom"
  ],
  "author": "Zack Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydux/hydux-data/issues"
  },
  "homepage": "https://github.com/hydux/hydux-data#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.56",
    "@types/pixi.js": "^4.7.3",
    "awesome-typescript-loader": "^3.4.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "debug": "^3.1.0",
    "del-cli": "^1.1.0",
    "jsdom": "^11.10.0",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "pre-commit": "^1.2.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "hydux": "^0.4.3",
    "pixi.js": "^4.7.3",
    "tslib": "^1.9.0"
  }
}
